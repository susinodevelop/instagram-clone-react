{
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "hooks": {
      "build": "node lib/initDb.js"
    }
  }
  